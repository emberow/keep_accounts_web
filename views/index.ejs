                    <label for="state">&ensp;&ensp;收入</label>
                    <input type="radio" id="income" name="state" value="income" ><br>
                </div>
                <b id="txtHint"></b><br>
                <p  id="add_button" onclick="add_record();">新增</p>
            </div>
        </div>
    </div>

