                    <label for="date" id="update_date_title">日期:</label>
                    <input type="date" id="update_date" value="2022-01-22"><br>
                </div>
                
                <div class="update_form_expense_income">
                    <label for="state">支出</label>
                    <input type="radio" id="update_expense" name="state" value="expense" checked>
                    <label for="state">&ensp;&ensp;收入</label>
                    <input type="radio" id="update_income" name="state" value="income" ><br>
                </div>

                <b id="update_txtHint"></b><br>
                <p  id="update_button" onclick="update_record();">修改</p>
                <p  id="delete_button" onclick="delete_record(); esc_button();">刪除</p>
                <!-- <input id="update_button" type="button" value="修改" onclick="update_record();">&ensp;&ensp; -->
            </div>
        </div>
    </div>
    

    <p id="accounting_table"></p>

    <!-- 引入jquery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="/script/index.js"></script>
